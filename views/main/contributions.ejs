<%- layout("layouts/boilerplate") %>
<body>
    <h2 class="mt-3 mb-3">This is contributions page</h2>
    <div class="row row-cols-lg-3 row-cols-mgd-2 row-cols-sm-1 d-flex flex-wrap">
        <% for(let dataset of userContributions){ %>
            <div class="card" >
                <div class="card-body">
                  <p class="card-text">
                    Color : <%=dataset.color%> <br><br>
                    Taste : <%=dataset.taste%> <br><br>
                    Ph Value : <%-dataset.phValue%> <br><br>
                    Density : <%=dataset.density%>
                </p>
                <p class=" date mb-2 "> Submitted At: <%= dataset.createdAt %></p>
                </div>
              </div>
        <% } %>
        </div>
</body>

